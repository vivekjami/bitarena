# Railway Ignore File
# Files and directories to exclude from deployment

# Node modules (will be installed on Railway)
node_modules/

# Development files
*.local
.env.local
.env.*.local

# Test files
**/*.test.ts
**/*.spec.ts
__tests__/
coverage/

# Build artifacts (will be built on Railway)
dist/

# Logs
logs/
*.log
npm-debug.log*
yarn-debug.log*
yarn-error.log*

# Editor directories
.vscode/
.idea/
*.swp
*.swo
*~

# OS files
.DS_Store
Thumbs.db

# Git
.git/
.gitignore

# Documentation
*.md
!README.md

# Scripts (except migrations)
scripts/*.ts
!scripts/migrate.ts
!scripts/seed.ts
